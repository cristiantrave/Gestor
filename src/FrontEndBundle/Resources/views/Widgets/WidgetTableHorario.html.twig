      <div class="panel panel-danger">
        <!-- Default panel contents -->
        <div class="panel-heading" style="text-align: center; font-weight: bold">HORARIO DE CLASES
        </div>
        <!-- Table -->
        <table id="tablaClase" class="table table-striped table-bordered" cellspacing="0" width="100%">
          <thead>
           <tr>
            {% for campo in cabeceras %}
              <th>{{campo}}</th>
            {% endfor %} 
          </tr>
        </thead>
        <tbody>
          {% for horario in filas %}
            <tr>
              {% for campo in horario %}

                <td>
                  {{campo}}
                </td>
              {% endfor %}
            </tr>
          {% endfor %} 
        </tbody>
      </table>
    </div>